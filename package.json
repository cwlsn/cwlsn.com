{
  "name": "cwlsn.com",
  "version": "1.0.0",
  "description": "Repo for my personal website",
  "main": "./public/app.js",
  "scripts": {
    "test": "xo && echo \"Tests are not valued at this time.\" && exit 1",
    "dev": "parcel ./src/index.html --open",
    "build": "parcel build ./src/index.html --out-dir public --public-url ./",
    "site": "git reset --hard && git checkout master && git pull && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cwlsn/cwlsn.com.git"
  },
  "keywords": [],
  "author": "Connor Wilson <me@cwlsn.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cwlsn/cwlsn.com/issues"
  },
  "homepage": "https://cwlsn.com",
  "xo": {
    "globals": [
      "document"
    ],
    "semicolon": false,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.8.3",
    "parcel-bundler": "^1.6.2"
  }
}
